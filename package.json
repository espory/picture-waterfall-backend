{
  "name": "picture-waterfall-backend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "commitizen": "^4.3.0",
    "egg": "^3",
    "egg-scripts": "^2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --port 8000 --daemon --title=egg-server-picture-waterfall-backend",
